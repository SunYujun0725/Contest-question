#include <stdio.h>

int main()
{
    int a;
    scanf("%d",&a);
    int b[a];
    for(int i=0;i<a;i++){
        scanf("%d",&b[i]);
    }
    int c,d;
    scanf("%d %d",&c,&d);
    int e[c];
    for(int j=0;j<c;j++){
        scanf("%d",&e[j]);
    }
    int sum=0;
    int j=0;
    for(int m=0;m<d;m++){
        while(j<c){
            sum+=b[e[j]+m];
            j++;
            
        }
        printf("%d",sum);
        printf(" ");
        j=0;
        sum=0;
        
    }
   
    return 0;
}
